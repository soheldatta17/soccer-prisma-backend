{
  "version": 2,
  "builds": [
    {
      "src": "index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/v1/(.*)",
      "dest": "index.ts"
    },
    {
      "src": "/docs",
      "dest": "index.ts"
    },
    {
      "src": "/swagger",
      "dest": "index.ts"
    },
    {
      "src": "/health",
      "dest": "index.ts"
    },
    {
      "src": "/readme",
      "dest": "index.ts"
    },
    {
      "src": "/",
      "dest": "index.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}

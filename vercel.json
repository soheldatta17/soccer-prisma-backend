{
  "version": 2,
  "routes": [
    {
      "src": "/v1/(.*)",
      "dest": "index.ts"
    },
    {
      "src": "/docs",
      "dest": "index.ts"
    },
    {
      "src": "/swagger",
      "dest": "index.ts"
    },
    {
      "src": "/health",
      "dest": "index.ts"
    },
    {
      "src": "/readme",
      "dest": "index.ts"
    },
    {
      "src": "/",
      "dest": "index.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "index.ts": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}
